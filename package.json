{
  "name": "sanji-router-ui",
  "author": "Zack Yang <zack9433@gmail.com> (https://github.com/zack9433)",
  "description": "sanji-router-ui",
  "version": "0.0.0-semantic-released",
  "main": "index",
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run test:once && npm run check-coverage"
    }
  },
  "scripts": {
    "prebuild": "rm -rf dist && mkdir dist",
    "build": "NODE_ENV=production webpack --config webpack.build.js --display-modules",
    "commit": "git-cz",
    "check-coverage": "istanbul check-coverage --statements 80 --branches 50 --functions 80 --lines 70",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "start": "NODE_ENV=development node server/dev-server.js",
    "test": "NODE_ENV=test karma start",
    "test:once": "karma start",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "devDependencies": {
    "angular-mocks": "~1.5.0",
    "autoprefixer-loader": "~3.2.0",
    "babel-core": "~5.8.34",
    "babel-eslint": "~4.1.6",
    "babel-loader": "~5.3.3",
    "babel-runtime": "~5.8.34",
    "codecov.io": "~0.1.6",
    "commitizen": "~2.5.0",
    "css-loader": "~0.23.1",
    "cz-conventional-changelog": "~1.1.5",
    "es5-shim": "~4.1.14",
    "es6-shim": "~0.28.1",
    "eslint": "~1.10.3",
    "eslint-loader": "~1.2.0",
    "extract-text-webpack-plugin": "~1.0.1",
    "file-loader": "~0.8.5",
    "ghooks": "~1.0.3",
    "html-webpack-plugin": "~1.7.0",
    "isparta-loader": "~1.0.0",
    "istanbul": "~0.4.2",
    "jquery": "~2.2.0",
    "karma": "~0.13.19",
    "karma-chai-plugins": "~0.6.1",
    "karma-coverage": "~0.5.3",
    "karma-es6-shim": "~0.2.0",
    "karma-mocha": "~0.2.0",
    "karma-mocha-reporter": "~1.1.5",
    "karma-phantomjs-launcher": "~1.0.0",
    "karma-sourcemap-loader": "~0.3.7",
    "karma-webpack": "~1.7.0",
    "ng-annotate-loader": "~0.1.0",
    "ng-cache-loader": "~0.0.15",
    "node-bourbon": "~4.2.3",
    "node-sass": "~3.4.2",
    "phantomjs": "~2.1.3",
    "raw-loader": "~0.5.1",
    "sass-loader": "~3.1.2",
    "semantic-release": "^4.3.5",
    "style-loader": "~0.13.0",
    "url-loader": "~0.5.7",
    "webpack": "~1.12.11",
    "webpack-dev-server": "~1.14.1",
    "webpack-notifier": "~1.2.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Sanji-IO/sanji-router-ui.git"
  },
  "bugs": {
    "url": "https://github.com/Sanji-IO/sanji-router-ui/issues"
  },
  "keywords": [
    "sanji",
    "router"
  ],
  "license": "MIT",
  "homepage": "https://github.com/Sanji/sanji-router-ui",
  "peerDependencies": {
    "toastr": "^2.1.2"
  },
  "dependencies": {
    "angular": "~1.5.0",
    "angular-ui-router": "~0.2.15",
    "sanji-logger-ui": "~1.1.0"
  }
}
