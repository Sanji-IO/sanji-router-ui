{
  "name": "sanji-router-ui",
  "author": "Zack Yang <zack9433@gmail.com> (https://github.com/zack9433)",
  "description": "sanji-router-ui",
  "version": "1.2.2",
  "main": "dist/sanji-router-ui.js",
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run test:once && npm run check-coverage"
    }
  },
  "scripts": {
    "prebuild": "rm -rf dist && mkdir dist",
    "build": "NODE_ENV=production webpack --config webpack.build.js --display-modules",
    "commit": "git-cz",
    "check-coverage": "istanbul check-coverage --statements 80 --branches 50 --functions 80 --lines 70",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "start": "NODE_ENV=development node server/dev-server.js",
    "test": "NODE_ENV=test karma start",
    "test:once": "karma start",
    "publish-latest": "publish-latest",
    "semantic-release": "semantic-release pre && npm publish && npm run publish-latest && semantic-release post"
  },
  "devDependencies": {
    "angular-mocks": "~1.5.0",
    "autoprefixer-loader": "~3.2.0",
    "babel-core": "^6.0.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.0.0",
    "babel-plugin-add-module-exports": "^0.2.0",
    "babel-plugin-transform-runtime": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-runtime": "^6.6.1",
    "codecov.io": "^0.1.6",
    "commitizen": "^2.8.0",
    "css-loader": "~0.23.1",
    "cz-conventional-changelog": "^1.1.5",
    "eslint": "^2.0.0",
    "eslint-loader": "^1.3.0",
    "extract-text-webpack-plugin": "~1.0.1",
    "file-loader": "~0.8.5",
    "ghooks": "^1.2.0",
    "html-webpack-plugin": "~1.7.0",
    "isparta-loader": "~2.0.0",
    "istanbul": "~0.4.2",
    "jquery": "~2.2.0",
    "karma": "~0.13.19",
    "karma-chai-plugins": "~0.7.0",
    "karma-coverage": "~1.0.0",
    "karma-mocha": "~0.2.0",
    "karma-mocha-reporter": "~2.0.0",
    "karma-phantomjs-launcher": "~1.0.0",
    "karma-sourcemap-loader": "~0.3.7",
    "karma-webpack": "~1.7.0",
    "ng-annotate-loader": "~0.1.0",
    "ng-cache-loader": "~0.0.15",
    "node-bourbon": "~4.2.3",
    "node-sass": "~3.7.0",
    "phantomjs": "~2.1.3",
    "publish-latest": "~1.1.2",
    "raw-loader": "~0.5.1",
    "sass-loader": "~3.2.0",
    "semantic-release": "^4.3.5",
    "style-loader": "~0.13.0",
    "toastr": "^2.1.2",
    "url-loader": "~0.5.7",
    "webpack": "^1.13.0",
    "webpack-dev-server": "~1.14.1",
    "webpack-notifier": "~1.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Sanji-IO/sanji-router-ui.git"
  },
  "bugs": {
    "url": "https://github.com/Sanji-IO/sanji-router-ui/issues"
  },
  "keywords": [
    "sanji",
    "router"
  ],
  "license": "MIT",
  "homepage": "https://github.com/Sanji-IO/sanji-router-ui",
  "dependencies": {
    "angular": "~1.5.0",
    "angular-ui-router": "~0.2.15",
    "sanji-logger-ui": "~1.2.2"
  }
}